{
    "name": "laravie/codex-filter",
    "description": "Content filtering for Laravie Codex",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Codex\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Codex\\Filter\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "igorw/get-in": "^1.0",
        "laravie/codex-common": "^1.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "php-http/guzzle7-adapter": "^0.1",
        "phpstan/phpstan": "0.12.x-dev",
        "phpunit/phpunit": "^8.4 || ^9.3.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
