{
    "name": "laravie/codex-filter",
    "description": "Content filtering for Laravie Codex",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Codex\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Codex\\Filter\\Tests\\": "tests/"
        }
    },
    "require": {
        "igorw/get-in": "^1.0",
        "laravie/codex-common": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
